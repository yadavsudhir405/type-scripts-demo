{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [false],
    "eofline": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
